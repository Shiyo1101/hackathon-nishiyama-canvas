{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**", "dist/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"clean": {
			"cache": false
		},
		"type-check": {
			"dependsOn": ["^type-check"]
		},
		"test": {
			"dependsOn": ["^build"]
		},
		"lint": {},
		"check": {}
	}
}
